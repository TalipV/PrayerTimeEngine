             xmlns:uraniumMaterial="clr-namespace:UraniumUI.Material.Controls;assembly=UraniumUI.Material"
             x:Class="PrayerTimeEngine.Views.SettingsHandlerPage">
    
    <NavigationPage.TitleView>
        <Label
            Text="Configuration"
            FontAttributes="Bold"
            FontSize="20"
            HorizontalTextAlignment="Center"
            VerticalTextAlignment="Center"/>
    </NavigationPage.TitleView>

    <ContentPage.Content>
        <uraniumMaterial:TabView x:Name="tabView" TabPlacement="Top">
        </uraniumMaterial:TabView>
    </ContentPage.Content>
</ContentPage>